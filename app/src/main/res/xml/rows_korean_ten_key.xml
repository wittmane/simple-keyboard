<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <Row
        latin:keyWidth="23.333%p" >
        <Key
            latin:keyWidth="15%p"
            latin:keySpec="!"
            latin:backgroundType="functional" />
        <switch>
            <case
                latin:showNumberRow="true">
                <!-- U+3163: "ㅣ" HANGUL LETTER I -->
                <Key
                    latin:keySpec="&#x3163;" />
                <!-- U+318D: "ㆍ" HANGUL LETTER ARAEA -->
                <Key
                    latin:keySpec="&#x318D;" />
                <!-- U+3161: "ㅡ" HANGUL LETTER EU -->
                <Key
                    latin:keySpec="&#x3161;" />
            </case>
            <default>
                <!-- U+3163: "ㅣ" HANGUL LETTER I -->
                <Key
                    latin:keySpec="&#x3163;"
                    latin:keyHintLabel="1"
                    latin:additionalMoreKeys="1" />
                <!-- U+318D: "ㆍ" HANGUL LETTER ARAEA -->
                <Key
                    latin:keySpec="&#x318D;"
                    latin:keyHintLabel="2"
                    latin:additionalMoreKeys="2" />
                <!-- U+3161: "ㅡ" HANGUL LETTER EU -->
                <Key
                    latin:keySpec="&#x3161;"
                    latin:keyHintLabel="3"
                    latin:additionalMoreKeys="3" />
            </default>
        </switch>
        <Key
            latin:keyWidth="15.001%p"
            latin:keySpec="&quot;"
            latin:additionalMoreKeys="&apos;,),("
            latin:backgroundType="functional" />
    </Row>

    <Row
        latin:keyWidth="23.333%p" >
        <Key
            latin:keyWidth="15%p"
            latin:keySpec="?"
            latin:backgroundType="functional" />
        <switch>
            <case
                latin:showNumberRow="true">
                <!-- U+3131: "ㄱ" HANGUL LETTER KIYEOK
                     U+314B: "ㅋ" HANGUL LETTER KHIEUKH -->
                <Key
                    latin:keySpec="&#x3131;&#x314B;|&#x3131;"
                    latin:keyLabelFlags="followKeyLetterRatio"/>
                <!-- U+3134: "ㄴ" HANGUL LETTER NIEUN
                     U+3139: "ㄹ" HANGUL LETTER RIEUL -->
                <Key
                    latin:keySpec="&#x3134;&#x3139;|&#x3134;"
                    latin:keyLabelFlags="followKeyLetterRatio"/>
                <!-- U+3137: "ㄷ" HANGUL LETTER TIKEUT
                     U+314C: "ㅌ" HANGUL LETTER THIEUTH -->
                <Key
                    latin:keySpec="&#x3137;&#x314C;|&#x3137;"
                    latin:keyLabelFlags="followKeyLetterRatio"/>
            </case>
            <default>
                <!-- U+3131: "ㄱ" HANGUL LETTER KIYEOK
                     U+314B: "ㅋ" HANGUL LETTER KHIEUKH -->
                <Key
                    latin:keySpec="&#x3131;&#x314B;|&#x3131;"
                    latin:keyHintLabel="4"
                    latin:additionalMoreKeys="4"
                    latin:keyLabelFlags="followKeyLetterRatio"/>
                <!-- U+3134: "ㄴ" HANGUL LETTER NIEUN
                     U+3139: "ㄹ" HANGUL LETTER RIEUL -->
                <Key
                    latin:keySpec="&#x3134;&#x3139;|&#x3134;"
                    latin:keyHintLabel="5"
                    latin:additionalMoreKeys="5"
                    latin:keyLabelFlags="followKeyLetterRatio"/>
                <!-- U+3137: "ㄷ" HANGUL LETTER TIKEUT
                     U+314C: "ㅌ" HANGUL LETTER THIEUTH -->
                <Key
                    latin:keySpec="&#x3137;&#x314C;|&#x3137;"
                    latin:keyHintLabel="6"
                    latin:additionalMoreKeys="6"
                    latin:keyLabelFlags="followKeyLetterRatio"/>
            </default>
        </switch>
        <switch>
            <case latin:mode="url" >
                <Key
                    latin:keyWidth="15.001%p"
                    latin:keySpec="/"
                    latin:keyStyle="settingsMoreKeysStyle" />
            </case>
            <case latin:mode="email" >
                <Key
                    latin:keyWidth="15.001%p"
                    latin:keySpec="\@"
                    latin:keyStyle="settingsMoreKeysStyle" />
            </case>
            <default>
                <Key
                    latin:keyWidth="15.001%p"
                    latin:keySpec=","
                    latin:keyLabelFlags="hasPopupHint"
                    latin:keyStyle="settingsMoreKeysStyle" />
            </default>
        </switch>
    </Row>

    <Row
        latin:keyWidth="23.333%p" >
        <Key
            latin:keyWidth="15%p"
            latin:keySpec="."
            latin:additionalMoreKeys="\\,,#,/,;,@,:,-,+,\\%,&amp;"
            latin:backgroundType="functional" />
        <switch>
            <case
                latin:showNumberRow="true">
                <!-- U+3142: "ㅂ" HANGUL LETTER PIEUP
                     U+314D: "ㅍ" HANGUL LETTER PHIEUPH -->
                <Key
                    latin:keySpec="&#x3142;&#x314D;|&#x3142;"
                    latin:keyLabelFlags="followKeyLetterRatio" />
                <!-- U+3145: "ㅅ" HANGUL LETTER SIOS
                     U+314E: "ㅎ" HANGUL LETTER HIEUH -->
                <Key
                    latin:keySpec="&#x3145;&#x314E;|&#x3145;"
                    latin:keyLabelFlags="followKeyLetterRatio" />
                <!-- U+3148: "ㅈ" HANGUL LETTER CIEUC
                     U+314A: "ㅊ" HANGUL LETTER CHIEUCH -->
                <Key
                    latin:keySpec="&#x3148;&#x314A;|&#x3148;"
                    latin:keyLabelFlags="followKeyLetterRatio" />
            </case>
            <default>
                <!-- U+3142: "ㅂ" HANGUL LETTER PIEUP
                     U+314D: "ㅍ" HANGUL LETTER PHIEUPH -->
                <Key
                    latin:keySpec="&#x3142;&#x314D;|&#x3142;"
                    latin:keyHintLabel="7"
                    latin:additionalMoreKeys="7"
                    latin:keyLabelFlags="followKeyLetterRatio" />
                <!-- U+3145: "ㅅ" HANGUL LETTER SIOS
                     U+314E: "ㅎ" HANGUL LETTER HIEUH -->
                <Key
                    latin:keySpec="&#x3145;&#x314E;|&#x3145;"
                    latin:keyHintLabel="8"
                    latin:additionalMoreKeys="8"
                    latin:keyLabelFlags="followKeyLetterRatio" />
                <!-- U+3148: "ㅈ" HANGUL LETTER CIEUC
                     U+314A: "ㅊ" HANGUL LETTER CHIEUCH -->
                <Key
                    latin:keySpec="&#x3148;&#x314A;|&#x3148;"
                    latin:keyHintLabel="9"
                    latin:additionalMoreKeys="9"
                    latin:keyLabelFlags="followKeyLetterRatio" />
            </default>
        </switch>
        <Key
            latin:keyWidth="15.001%p"
            latin:keyStyle="deleteKeyStyle" />
    </Row>

    <Row
        latin:keyWidth="23.333%p" >
        <Key
            latin:keyWidth="15%p"
            latin:keyStyle="toSymbolKeyStyle" />
        <switch>
            <case
                latin:languageSwitchKeyEnabled="true" >
                <Key
                    latin:keyStyle="languageSwitchKeyStyle" />
            </case>
            <default>
                <Spacer />
            </default>
        </switch>
        <switch>
            <case
                latin:showNumberRow="true">
                <!-- U+3147: "ㅇ" HANGUL LETTER IEUNG
                     U+3141: "ㅁ" HANGUL LETTER MIEUM -->
                <Key
                    latin:keySpec="&#x3147;&#x3141;|&#x3147;"
                    latin:keyLabelFlags="followKeyLetterRatio" />
            </case>
            <default>
                <!-- U+3147: "ㅇ" HANGUL LETTER IEUNG
                     U+3141: "ㅁ" HANGUL LETTER MIEUM -->
                <Key
                    latin:keySpec="&#x3147;&#x3141;|&#x3147;"
                    latin:keyHintLabel="0"
                    latin:additionalMoreKeys="0"
                    latin:keyLabelFlags="followKeyLetterRatio" />
            </default>
        </switch>
        <Key
            latin:keyStyle="numSpaceKeyStyle" />
        <Key
            latin:keyWidth="15.001%p"
            latin:keyStyle="enterKeyStyle" />
    </Row>
</merge>
